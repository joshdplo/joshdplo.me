---
const { data } = Astro.props;
const keepId = data.id;
const keepData = data.data;
---

<a class="card" href={`/keeps/${keepId}`}>
  {keepData.title}
</a>

<style>
  .card {
    position: relative;
    display: inline-block;
    padding: 0.3em;
    border: 2px solid var(--font-color);
  }
</style>
